{
	"compilerOptions": {
    "lib": ["es2017"],
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
    "target": "es2017"
	},
	"include": ["src/**/*.ts", "serverless.ts"],
	"exclude": [
		"node_modules/**/*",
		".serverless/**/*",
		".webpack/**/*",
		"_warmup/**/*",
		".vscode/**/*"
	]
}
